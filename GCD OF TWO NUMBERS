#include <stdio.h>
int main()
{
    int a,b,gcd,i;
    printf("Enter the first digit:");
    scanf("%d",&a);
    printf("Enter the second digit:");
    scanf("%d",&b);
    for (i = 1; i <= a && i <= b; i++)
    {
        if (a % i == 0 && b % i == 0)
        {
            gcd = i;
        }
    }
    printf("The GCD of %d and %d is : %d\n",a,b, gcd);
    return 0;
}
